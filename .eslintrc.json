{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": ["import", "react", "react-hooks", "prettier"],
    "rules": {
        "strict": 0,
        "no-var": ["error"],
        "semi": [2, "never"],
        "quotes": ["warn", "double"],
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
